// database
datasource db {
    provider = "mongodb"
    url      = env("MONGODB_URI")
}

// generator
generator client {
    provider             = "prisma-client-py"
    recursive_type_depth = 5
}

model User {
    id    String  @id @default(auto()) @map("_id") @db.ObjectId
    email String  @unique
    name  String?
}
